{
  "name": "ng-tree-transfer",
  "version": "0.0.3",
  "description": "tree-transfer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzhd/ng-tree-transfer.git"
  },
  "keywords": [
    "tree-transfer",
    "tree",
    "ng-zorro-antd",
    "angular",
    "ng-tree-transfer",
    "nz-tree-transfer",
    "transfer"
  ],
  "author": "lzhd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lzhd/ng-tree-transfer/issues"
  },
  "homepage": "https://lzhd.github.io/ng-tree-transfer",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "site:build": "ng build --prod --build-optimizer",
    "site:gh": "ng build --prod --build-optimizer --base-href /ng-tree-transfer/ && gh-pages -d ./dist",
    "build": "node scripts/build.js",
    "release:next": "npm run build && cd publish && npm publish --access public --tag next",
    "release": "npm run build && cd publish && npm publish --access public"
  },
  "peerDependencies": {
    "lodash": ">=4.17.11",
    "ng-zorro-antd": ">=7.0.0"
  },
  "devDependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "ng-packagr": "^5.1.0",
    "ng-zorro-antd": "^7.3.3"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "lib/index.ts"
    },
    "dest": "./publish"
